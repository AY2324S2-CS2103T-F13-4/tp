@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":AddressBookParser" as AddressBookParser LOGIC_COLOR
participant ":StarCommandParser" as StarCommandParser LOGIC_COLOR
participant "d:StarCommand" as StarCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("star 1 s/1")
activate LogicManager

LogicManager -> AddressBookParser : parseCommand("star 1 s/1")
activate AddressBookParser

create StarCommandParser
AddressBookParser -> StarCommandParser
activate StarCommandParser

StarCommandParser --> AddressBookParser
deactivate StarCommandParser

AddressBookParser -> StarCommandParser : parse("1 s/1")
activate StarCommandParser

create StarCommand
StarCommandParser -> StarCommand
activate StarCommand

create Star
StarCommandParser -> Star
activate Star

Star --> StarCommandParser
deactivate Star

StarCommand --> StarCommandParser :
deactivate StarCommand

StarCommandParser --> AddressBookParser : d
deactivate StarCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
StarCommandParser -[hidden]-> AddressBookParser
destroy StarCommandParser
deactivate StarCommandParser

AddressBookParser --> LogicManager : d
deactivate AddressBookParser

LogicManager -> StarCommand : execute(m)
activate StarCommand

StarCommand -> Model : updateFilteredPersonList(Star)
activate Model

Model --> StarCommand : Updated filtered person list
deactivate Model

create CommandResult
StarCommand -> CommandResult
activate CommandResult

CommandResult --> StarCommand
deactivate CommandResult

StarCommand --> LogicManager : r
deactivate StarCommand

[<--LogicManager
deactivate LogicManager
@enduml
