@startuml
!include style.puml
skinparam ArrowFontStyle plain

box Logic LOGIC_COLOR_T1
participant ":LogicManager" as LogicManager LOGIC_COLOR
participant ":ClassMonitorParser" as ClassMonitorParser LOGIC_COLOR
participant ":FindStarsLessThanCommandParser" as FindStarsLessThanCommandParser LOGIC_COLOR
participant "d:FindStarsLessThanCommand" as FindStarsLessThanCommand LOGIC_COLOR
participant "r:CommandResult" as CommandResult LOGIC_COLOR
end box

box Model MODEL_COLOR_T1
participant "m:Model" as Model MODEL_COLOR
end box

[-> LogicManager : execute("findStarsLT 1")
activate LogicManager

LogicManager -> ClassMonitorParser : parseCommand("findStarsLT 1")
activate ClassMonitorParser

create FindStarsLessThanCommandParser
ClassMonitorParser -> FindStarsLessThanCommandParser
activate FindStarsLessThanCommandParser

FindStarsLessThanCommandParser --> ClassMonitorParser
deactivate FindStarsLessThanCommandParser

ClassMonitorParser -> FindStarsLessThanCommandParser : parse("1")
activate FindStarsLessThanCommandParser

create StarsLessThanPredicate
FindStarsLessThanCommandParser -> StarsLessThanPredicate
activate StarsLessThanPredicate

StarsLessThanPredicate --> FindStarsLessThanCommandParser
deactivate StarsLessThanPredicate

create FindStarsLessThanCommand
FindStarsLessThanCommandParser -> FindStarsLessThanCommand
activate FindStarsLessThanCommand

FindStarsLessThanCommand --> FindStarsLessThanCommandParser :
deactivate FindStarsLessThanCommand

FindStarsLessThanCommandParser --> ClassMonitorParser : d
deactivate FindStarsLessThanCommandParser
'Hidden arrow to position the destroy marker below the end of the activation bar.
FindStarsLessThanCommandParser -[hidden]-> ClassMonitorParser
destroy FindStarsLessThanCommandParser
deactivate FindStarsLessThanCommandParser

ClassMonitorParser --> LogicManager : d
deactivate ClassMonitorParser

LogicManager -> FindStarsLessThanCommand : execute(m)
activate FindStarsLessThanCommand

FindStarsLessThanCommand -> Model : updateFilteredStudentList(predicate)
activate Model

Model --> FindStarsLessThanCommand : Updated filtered student list
deactivate Model

create CommandResult
FindStarsLessThanCommand -> CommandResult
activate CommandResult

CommandResult --> FindStarsLessThanCommand
deactivate CommandResult

FindStarsLessThanCommand --> LogicManager : r
deactivate FindStarsLessThanCommand

[<--LogicManager
deactivate LogicManager
@enduml
